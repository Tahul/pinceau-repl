{
  "name": "pinceau-repl",
  "version": "0.0.1",
  "description": "Vue component for editing Vue components with Pinceau",
  "author": "Evan You",
  "license": "MIT",
  "homepage": "https://github.com/Tahul/pinceau-repl#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tahul/pinceau-repl.git"
  },
  "bugs": {
    "url": "https://github.com/Tahul/pinceau-repl/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/repl.d.ts",
      "require": "./dist/ssr-stub.js",
      "import": "./dist/pinceau-repl.js"
    },
    "./monaco-editor": {
      "types": "./dist/src/editor/MonacoEditor.vue.d.ts",
      "require": null,
      "import": "./dist/pinceau-repl-monaco-editor.js"
    },
    "./style.css": "./dist/style.css",
    "./dist/style.css": "./dist/style.css"
  },
  "main": "dist/ssr-stub.js",
  "module": "dist/pinceau-repl.js",
  "types": "dist/repl.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --force",
    "dev:site": "vite -c site.config.ts --force",
    "build": "vite build",
    "build:site": "vite -c site.config.ts build",
    "build:types": "vue-tsc -p tsconfig.build.json && api-extractor run -c api-extractor.json && rimraf dist/src",
    "prepublishOnly": "npm run build && npm run build-types",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix"
  },
  "contributors": [
    "YaÃ«l Guilloux <yael.guilloux@gmail.com>"
  ],
  "peerDependencies": {
    "vue": "^3.2.37"
  },
  "dependencies": {
    "@babel/parser": "^7.21.2",
    "@babel/types": "^7.21.2",
    "@microsoft/api-extractor": "^7.34.4",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@volar/monaco": "1.3.0-alpha.3",
    "@volar/vue-language-service": "1.2.2-alpha.0",
    "@vue/compiler-sfc": "^3.2.47",
    "@vueuse/core": "^9.13.0",
    "babylon": "^6.18.0",
    "fflate": "^0.7.4",
    "hash-sum": "^2.0.0",
    "jiti": "^1.17.1",
    "js-beautify": "^1.14.7",
    "monaco-editor": "^0.36.1",
    "monaco-editor-core": "^0.36.1",
    "monaco-editor-textmate": "^4.0.0",
    "monaco-textmate": "^3.0.1",
    "monaco-volar": "^0.1.1",
    "nanoid": "^4.0.1",
    "onigasm": "^2.2.5",
    "pathe": "^1.1.0",
    "pinceau": "latest",
    "postcss": "^8.4.21",
    "postcss-custom-properties": "^13.1.4",
    "postcss-dark-theme-class": "^0.7.3",
    "postcss-nested": "^6.0.1",
    "rimraf": "^4.1.2",
    "source-map-js": "^1.0.2",
    "sucrase": "^3.29.0",
    "typescript": "^4.9.5",
    "unenv": "^1.2.1",
    "untyped": "^1.2.2",
    "vite": "^4.1.4",
    "vite-plugin-vue-component-preview": "^1.1.5",
    "vscode-uri": "^3.0.7",
    "vue-tsc": "^1.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.3",
    "eslint": "^8.35.0",
    "execa": "^5.1.1",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "vue": "^3.2.47"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "monaco-editor-core": "0.35.0"
      }
    }
  }
}
